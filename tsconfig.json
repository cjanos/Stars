{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es6",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "noEmit": false,
    "jsx": "react",
    "lib": ["es6", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
  },
  "include": ["./src"],
  "exclude": ["node_modules", "build"]
}